<article id="0" class="container-card">
      <div class="card">
        <div class="card-body">
          <h4 class="card-title">Nombre</h4>
          <p class="card-text">Precio</p>
        </div>
        <div class="card-img"><p>Imagen</p></div>
      </div>
    </article>
{{#each productos}}
  <article id={{this.id}} class="container-card">
      <div class="card">
        <div class="card-body">
          <h4 class="card-title">{{this.nombre}}</h4>
          <p class="card-text">{{this.precio}}</p>
        </div>
        <img class="card-img" src={{this.imagen}} alt={{this.nombre}}/>
      </div>
    </article>
{{/each}}