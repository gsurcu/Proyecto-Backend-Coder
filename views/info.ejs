<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Info</title>
</head>
<body>
  <main>
    <% for (const prop in data) { %>
      <div>
        <span><b><%= prop %>:</b> </span>
        <% if (typeof(data[prop]) == 'object') { %>
          <% const obj = data[prop] %> 
          <% for (const prop1 in obj) { %>
            <div>
              <span><%= prop1 %>: </span>
              <span><%= obj[prop1] %> </span>
            </div>
          <% } %> 
        <% } else { %>
        <span><%= data[prop] %> </span>
        <% } %> 
      </div>
    <% } %> 
  </main>
</body>
</html>